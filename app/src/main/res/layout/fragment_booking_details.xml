<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:id="@+id/bookingDetailsFragment"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="?android:windowBackground"
	android:fitsSystemWindows="true">
	
	
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical">
		
		<androidx.constraintlayout.widget.ConstraintLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent">

			<include
				android:id="@+id/booking_itinerary_recyclerview"
				layout="@layout/booking_itinerary_recyclerview"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintEnd_toEndOf="parent"
				app:layout_constraintHorizontal_bias="0.0"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				app:layout_constraintVertical_bias="0.078" />
			
			
			<include
				android:id="@+id/includebtnNvBookingDetails"
				layout="@layout/activity_booking_details" />
			
			
			<androidx.constraintlayout.widget.Guideline
				android:id="@+id/guideline8"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:orientation="vertical"
				app:layout_constraintGuide_begin="20dp" />
		</androidx.constraintlayout.widget.ConstraintLayout>
	
	
	</LinearLayout>

</FrameLayout>