    <style name="MyProgressDialogStyle" parent="Theme.MaterialComponents.Light.Dialog.Alert">
        <item name="colorAccent">@android:color/white</item>
        <item name="android:textColor">#000000</item>
        <item name="android:windowTitleStyle">@null</item>
    </style>

    <style name="appProgressbar" parent="AppTheme">
        <item name="colorControlActivated">@color/blue</item>
        <item name="android:indeterminateTint">@color/blue</item>
    </style>

    <style name="TextTitle">
        <item name="android:textSize">@dimen/_28sdp</item>
        <item name="android:fontFamily">@font/poppins_bold</item>
        <item name="android:textColor">@color/blackColor</item>
    </style>
    <style name="EditText">
        <item name="android:textSize">@dimen/_12sdp</item>
        <item name="android:padding">@dimen/_8sdp</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
    </style>

    <style name="ExtraSmallFont">
        <item name="android:textSize">@dimen/_10sdp</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
    </style>
    
    

